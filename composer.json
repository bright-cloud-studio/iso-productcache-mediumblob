{
    "name":"bright-cloud-studio/iso-productcache-mediumblob",
    "description":"Converts Contao's Isotope tl_iso_productcache.products field from 'blob' to 'mediumblob' to store more information.",
    "homepage": "http://brightcloudstudio.com/",
    "keywords":["contao", "isotope", "mediumblob", "blob", "bright", "cloud", "studio"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Bright Cloud Studio",
            "homepage":"https://brightcloudstudio.com"
        }
    ],
    "require":{
        "php": "^8.0",
        "contao/core-bundle": "^4.13"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\": [
                "system/modules/iso_productcache_mediumblob/library/Bcs"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/iso_productcache_mediumblob": "system/modules/iso_productcache_mediumblob"
            }
        }
    },
    "minimum-stability": "dev"
}
